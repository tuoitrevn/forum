.tuto-tag{background:#39C;border-radius:3px;color:#FFF;font-weight:700;padding:2px 8px}
.tuto-content + br,.tuto-footer + br,.tuto-header + br{display:none}
.tuto-content,.tuto-footer,.tuto-header{border-spacing:0;width:100%}
.tuto-header a,.tuto-header font{color:#FFF!important}
.tuto-header{background:#39C;border:1px solid #39C;border-radius:3px 3px 0 0;color:#FFF;font-family:"Trebuchet MS",Arial,Verdana,Sans-serif;padding:3px}
.tuto-logo,.tuto-type{height:75px;position:relative;text-align:center;width:150px}
.tuto-title{font-size:22px;text-align:center}
.tuto-logo:after{-moz-osx-font-smoothing:grayscale;-webkit-font-smoothing:antialiased;content:"\f086";font-family:FontAwesome;font-size:49px;font-style:normal;font-variant:normal;font-weight:normal;line-height:1;text-rendering:auto}
.announcement .tuto-logo:after{content:"\f0a1"}
.tutorial .tuto-logo:after{content:"\f0d0"}
.rules .tuto-logo:after{content:"\f071"}
.review .tuto-logo:after{content:"\f046"}
.graphic .tuto-logo:after{content:"\f1fc"}
.tuto-type:after{background:#FFF;border-radius:0 0 3px 3px;color:#39C;content:"Template";display:inline-block;font-size:12px;font-weight:700;padding:3px 9px;position:absolute;right:20px;top:-3px}
.announcement .tuto-type:after{content:"Announcement"}
.tutorial .tuto-type:after{color:#8B5;content:"Tutorial"}
.rules .tuto-type:after{color:#ed765e;content:"Important"}
.review .tuto-type:after{color:#5AB584;content:"Review"}
.graphic .tuto-type:after{color:#e06b59;content:"Graphic Design"}
.tuto-content{background:#FFF;border:1px solid #39C;border-bottom:3px dashed #CCC!important;padding:3px}
.tuto-footer{background:#FFF;border:1px solid #39C;border-bottom-width:5px;border-radius:0 0 3px 3px;border-top:none}
.tuto-credit{padding:10px 6px;position:relative;text-align:justify;vertical-align:middle}
.tuto-brand{height:60px;padding:6px 0;text-align:center;vertical-align:bottom;width:120px}
.tuto-brand:before{color:#39C;content:"Forum"}
.tuto-brand:after{color:#333;content:"otion.com"}
.tuto-credit:after{-moz-osx-font-smoothing:grayscale;-webkit-font-smoothing:antialiased;bottom:20px;color:#39C;content:"\f086";font-family:FontAwesome;font-size:49px;font-style:normal;font-variant:normal;font-weight:normal;line-height:1;position:absolute;right:-85px;text-rendering:auto}
.tuto-header.tutorial{background-color:#8B5;border-color:#8B5}
.tutorial + .tuto-content,.tutorial + .tuto-content + .tuto-footer,.tutorial + br + .tuto-content,.tutorial + br + .tuto-content + br + .tuto-footer{border-color:#8B5}
.tuto-header.rules{background-color:#ed765e;border-color:#ed765e}
.rules + .tuto-content,.rules + .tuto-content + .tuto-footer,.rules + br + .tuto-content,.rules + br + .tuto-content + br + .tuto-footer{border-color:#ed765e}
.tuto-header.review{background-color:#5AB584;border-color:#5AB584}
.review + .tuto-content,.review + .tuto-content + .tuto-footer,.review + br + .tuto-content,.review + br + .tuto-content + br + .tuto-footer{border-color:#5AB584}
.sticker-points img,.tuto-content table[style="width:100%;"] td[style="width:50%;text-align:center;"] img{vertical-align:middle}
.tuto-header.graphic{background-color:#e06b59;border-color:#e06b59}
.graphic + .tuto-content,.graphic + .tuto-content + .tuto-footer,.graphic + br + .tuto-content,.graphic + br + .tuto-content + br + .tuto-footer{border-color:#e06b59}

